		"express": "^4.16.3",
		"express-session": "^1.15.6",
		"extract-text-webpack-plugin": "3.0.2",
		"file-loader": "1.1.5",
		"fs-extra": "3.0.1",
		"html-webpack-plugin": "2.29.0",
		"jest": "^23.4.1",
		"knex": "^0.14.6",
		"lodash": "^4.17.10",
		"nodemon": "^1.18.2",
		"object-assign": "4.1.1",
		"pg": "^7.4.3",
		"postcss-flexbugs-fixes": "3.2.0",
		"postcss-loader": "2.0.8",
		"promise": "8.0.1",
		"prop-types": "^15.6.2",
		"raf": "3.4.0",
